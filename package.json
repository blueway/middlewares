{
  "name": "@nanoexpress/middlewares",
  "version": "0.0.4",
  "description": "Middleware packages for nanoexpress",
  "type": "module",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/dalisoft"
  },
  "scripts": {
    "lint:prettier": "prettier static/*.js && prettier body-parser/*.js && prettier graphql/*.js && prettier redoc/*.js",
    "lint:eslint": "eslint static/*.js && eslint body-parser/*.js && eslint graphql/*.js && eslint redoc/*.js",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "cleanup-cjs": "rm -rf **/cjs/*.cjs.js && rm -rf **/cjs/*.cjs.js.map",
    "build": "npm run cleanup-cjs && rollup -c",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "engines": {
    "node": ">=12.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanoexpress/middlewares.git"
  },
  "keywords": [
    "nanoexpress",
    "middlewares"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nanoexpress/middlewares/issues"
  },
  "homepage": "https://github.com/nanoexpress/middlewares#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "lerna": "^3.22.1",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "rollup": "^2.3.3"
  },
  "peerDependencies": {
    "graphql": "^15.0.0"
  }
}
